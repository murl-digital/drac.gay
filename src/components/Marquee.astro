<style>
    @keyframes marquee {
        from {
            transform: translate(0%);
        }

        to {
            transform: translate(-100%);
        }
    }
</style>
<marquee-container class=":uno: flex whitespace-nowrap overflow-hidden gap-15">
    {
        [0, 1].map((item) => (
            <marquee-item class="animate-[marquee_10s_linear_infinite]">
                <slot />
            </marquee-item>
        ))
    }
</marquee-container>
